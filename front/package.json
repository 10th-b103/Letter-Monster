{
  "name": "front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "local": "env-cmd -f .env.local vite --mode local",
    "dev": "env-cmd -f .env.development vite --mode development",
    "prod": "env-cmd -f .env.production vite --mode production",
    "build": "env-cmd -f .env.local tsc && vite build --mode local",
    "build:dev": "env-cmd -f .env.development tsc && vite build --mode development",
    "build:prod": "env-cmd -f .env.production tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@react-three/fiber": "^8.16.1",
    "@tanstack/react-query": "^5.29.2",
    "@tanstack/react-query-devtools": "^5.29.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/three": "^0.163.0",
    "@types/uuid": "^9.0.8",
    "axios": "^1.6.8",
    "env-cmd": "^10.1.0",
    "i18next": "^23.11.1",
    "konva": "^9.3.6",
    "node-sass": "^9.0.0",
    "npm": "^10.5.2",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-i18next": "^14.1.0",
    "react-konva": "^18.2.10",
    "react-router-dom": "^6.22.3",
    "react-uuid": "^2.0.0",
    "three": "^0.163.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-color": "^3.0.12",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "sass": "^1.75.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
